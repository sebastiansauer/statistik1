@article{aren_relationship_2020,
	title = {Relationship between risk aversion, risky investment intention, investment choices: Impact of personality traits and emotion},
	volume = {49},
	issn = {0368-492X, 0368-492X},
	url = {https://www.emerald.com/insight/content/doi/10.1108/K-07-2019-0455/full/html},
