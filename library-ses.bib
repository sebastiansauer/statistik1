	file = {Rohrer&Arslan_(2021)Precise Answers to Vague Questions - Issues With Interactions.pdf:/Users/sebastiansaueruser/Literatur/Rohrer&Arslan_(2021)Precise Answers to Vague Questions - Issues With Interactions.pdf:application/pdf},
}

@article{navarro_if_2021,
	title = {If Mathematical Psychology Did Not Exist We Might Need to Invent It: A Comment on Theory Building in Psychology},
