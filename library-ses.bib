	langid = {english},
	note = {Publisher: Nature Publishing Group},
	keywords = {Human behaviour, Science, technology and society},
	file = {Hoogeveen et al_(2022)The Einstein effect provides global evidence for scientific source credibility effects and the influence of religiosity.pdf:/Users/sebastiansaueruser/Literatur/Hoogeveen et al_(2022)The Einstein effect provides global evidence for scientific source credibility effects and the influence of religiosity.pdf:application/pdf},
