	author = {Rosendahl, Erik and Lindelöf, Nina and Littbrand, Håkan and Yifter-Lindgren, Elinor and Lundin-Olsson, Lillemor and Håglin, Lena and Gustafson, Yngve and Nyberg, Lars and Lindelof, N and Haglin, L},
