	file = {Bergh et al_(2021)A tutorial on Bayesian multi-model linear regression with BAS and JASP.pdf:/Users/sebastiansaueruser/Literatur/Bergh et al_(2021)A tutorial on Bayesian multi-model linear regression with BAS and JASP.pdf:application/pdf},
