	file = {Bender et al_(2021)On the Dangers of Stochastic Parrots - Can Language Models Be Too Big - &#x1f99c\;.pdf:/Users/sebastiansaueruser/Literatur/Bender et al_(2021)On the Dangers of Stochastic Parrots - Can Language Models Be Too Big - &#x1f99c\;.pdf:application/pdf},
