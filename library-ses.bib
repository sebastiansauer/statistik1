@incollection{Pfaff2013,
	location = {Pfaff, Holger: Universität Köln; Institut für Medizinsoziologie, Vorsorgungsforschung und Rehabilitationswissenschaft (Germany)},
	title = {Optionsstress und Zeitdruck},
	isbn = {978-3-658-01444-5},
	url = {http://search.ebscohost.com/login.aspx?direct=true&db=pdx&AN=0270079&lang=de&site=eds-live},
	abstract = {Die Bedeutung von Optionsstress und Zeitdruck in der modernen Arbeitswelt wird erörtert. Von Optionsstress wird gesprochen, wenn eine Person in einer Situation, die sich durch viele oder komplexe Optionen auszeichnet, zur Einschätzung gelangt, dass diese Situation ihre Ressourcen beansprucht oder übersteigt und ihr Wohlbefinden gefährdet. Optionsstress wird als ein Grundproblem des Menschen dargestellt, das durch die Modernisierung der Gesellschaft an Bedeutung gewinnt. Des Weiteren wird die These einer zweigeteilten Arbeitswelt entfaltet: In der strukturierten Arbeitswelt werden standardisierte Aufgaben in strukturierter Form abgearbeitet, so dass Optionsmangel das vorherrschende Merkmal ist. Die unstrukturierte Arbeitswelt zeichnet sich hingegen durch geringe Standardisierung, geringe Formalisierung und wenig festes Wissen sowie große Optionsräume aus. Konsequenzen dieser Zweiteilung für die Wissensarbeiter und andere Arbeitstätige werden verdeutlicht. In der Folge werden zum Verhältnis zwischen Optionsstress und Zeitdruck zwei Thesen aufgestellt: Optionslast führt zu Zeitdruck, Zeitdruck führt zu Optionsstress. Zur Eindämmumg von Optionsstress werden drei Maßnahmen genannt: (1) individuelles Training der Fähigkeit zu Optionsstressreduktion und Mehroptionsfähigkeit, (2) Strategien der sozialen Entscheidungsentlastung und (3) Strategien der sozialen Optionsreduktion.},
	pages = {112--143},
	booktitle = {Immer schneller, immer mehr. Psychische Belastung bei Wissens- und Dienstleistungsarbeit {TT} - Option stress and time pressure},
	publisher = {Springer {VS}},
	author = {Pfaff, Holger},
	editor = {Junghanns, Gisa and Morschhäuser, Martina and und Arbeitsmedizin, Bundesanstalt für Arbeitsschutz},
	date = {2013},
	keywords = {*Arbeitsbelastung, *Arbeitsplatzmerkmale, *Arbeitsplatzqualität, *Beruflicher Stress, *Job Characteristics, *Occupational Stress, *Quality of Work Life, *Time Management, *Work Load, *Zeitmanagement, Arbeitnehmereinstellungen und Arbeitszufriedenheit, Arbeitsbedingungen und Arbeitssicherheit, Bewältigungsverhalten, Coping Behavior, option stress \& time pressure in modern world of w, Personnel Attitudes \& Job Satisfaction, Strategien, Strategies, Working Conditions \& Industrial Safety},
