	keywords = {Humans, Adult, Aged, Male, Middle Aged, Arousal, Meditation, Female, Young Adult, Depression, Depression: psychology, Personality Inventory, Personality Inventory: statistics \& numerical data, Psychometrics, Anxiety, Heart Rate, Arousal: physiology, Anxiety: psychology, Meditation: psychology, Galvanic Skin Response, Galvanic Skin Response: physiology, Heart Rate: physiology, Sympathetic Nervous System, Anxiety: physiopathology, Depression: physiopathology, Fibromyalgia, Fibromyalgia: physiopathology, Fibromyalgia: psychology, Fibromyalgia: therapy, Psychophysiologic Disorders, Psychophysiologic Disorders: physiopathology, Psychophysiologic Disorders: psychology, Psychophysiologic Disorders: therapy, Skin Temperature, Skin Temperature: physiology, Sympathetic Nervous System: physiopathology},
