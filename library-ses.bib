@article{Shanks2013,
	title = {Priming Intelligent Behavior: An Elusive Phenomenon},
	volume = {8},
	url = {http://dx.doi.org/10.1371%2Fjournal.pone.0056515},
	abstract = {{\textless}p{\textgreater}Can behavior be unconsciously primed via the activation of attitudes, stereotypes, or other concepts? A number of studies have suggested that such priming effects can occur, and a prominent illustration is the claim that individuals' accuracy in answering general knowledge questions can be influenced by activating intelligence-related concepts such as {\textless}italic{\textgreater}professor{\textless}/italic{\textgreater} or {\textless}italic{\textgreater}soccer hooligan{\textless}/italic{\textgreater}. In 9 experiments with 475 participants we employed the procedures used in these studies, as well as a number of variants of those procedures, in an attempt to obtain this intelligence priming effect. None of the experiments obtained the effect, although financial incentives did boost performance. A Bayesian analysis reveals considerable evidential support for the null hypothesis. The results conform to the pattern typically obtained in word priming experiments in which priming is very narrow in its generalization and unconscious (subliminal) influences, if they occur at all, are extremely short-lived. We encourage others to explore the circumstances in which this phenomenon might be obtained.{\textless}/p{\textgreater}},
	pages = {e56515},
	number = {4},
