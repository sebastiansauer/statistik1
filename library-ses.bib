@article{wypych_psychological_2022,
	title = {Psychological toll of hate speech: The role of acculturation stress in the effects of exposure to ethnic slurs on mental health among Ukrainian immigrants in Poland},
	issn = {1939-0106},
	doi = {10.1037/cdp0000522},
	shorttitle = {Psychological toll of hate speech},
	abstract = {Objectives: Exposure to hate speech can have serious consequences for the mental health of immigrants and minority groups. However, not much is known about the process by which this effect takes place and to what extent it is independent of the effects of other forms of discrimination on health. The present study aimed to investigate whether acculturation stress mediates the relationship between exposure to hate speech and mental health and whether the effect would hold after controlling for experienced discrimination. Method: An online survey was conducted among Ukrainian immigrants living in Poland (N = 726) in order to investigate the relation between exposure to hate speech, acculturation stress, and mental health. Mediation analyses were used to test whether exposure to hateful language predicts posttraumatic stress disorder ({PTSD}) and depression symptoms and whether these effects are mediated by acculturation stress while controlling for experiences of other forms of discrimination. Results: Exposure to hate speech predicted {PTSD} and depression symptoms. Both effects were mediated by acculturation stress and were significant after controlling for experienced discrimination. Conclusions: The study provides evidence for the existence of a relation between exposure to hate speech among migrants and mental health problems. The study also provides support for a potential mechanism of this effect: acculturation stress and evidence for the fact that the effect holds over and above the effect of discrimination. ({PsycInfo} Database Record (c) 2022 {APA}, all rights reserved)},
	pages = {No Pagination Specified--No Pagination Specified},
	journaltitle = {Cultural Diversity and Ethnic Minority Psychology},
	author = {Wypych, Michał and Bilewicz, Michał},
	date = {2022},
	note = {Place: {US}
Publisher: Educational Publishing Foundation},
	keywords = {Mental Health, Stress, Hate, Acculturation, Ethnic Identity, Exposure, Foreign Language Translation, Immigration, Major Depression, Oral Communication, Posttraumatic Stress Disorder},
	file = {Snapshot:/home/sebastian/Documents/Zotero/storage/Y5Z59BSN/2022-23266-001.html:text/html},
}

