@article{Urrila2017,
	title = {Sleep habits, academic performance, and the adolescent brain structure.},
	volume = {7},
	issn = {2045-2322},
	url = {http://www.nature.com/articles/srep41678 http://www.ncbi.nlm.nih.gov/pubmed/28181512 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5299428},
	doi = {10.1038/srep41678},
	abstract = {Here we report the first and most robust evidence about how sleep habits are associated with regional brain grey matter volumes and school grade average in early adolescence. Shorter time in bed during weekdays, and later weekend sleeping hours correlate with smaller brain grey matter volumes in frontal, anterior cingulate, and precuneus cortex regions. Poor school grade average associates with later weekend bedtime and smaller grey matter volumes in medial brain regions. The medial prefrontal - anterior cingulate cortex appears most tightly related to the adolescents' variations in sleep habits, as its volume correlates inversely with both weekend bedtime and wake up time, and also with poor school performance. These findings suggest that sleep habits, notably during the weekends, have an alarming link with both the structure of the adolescent brain and school performance, and thus highlight the need for informed interventions.},
	pages = {41678},
	journaltitle = {Scientific reports},
	author = {Urrila, Anna S and Artiges, Eric and Massicotte, Jessica and Miranda, Ruben and Vulser, Hélène and Bézivin-Frere, Pauline and Lapidaire, Winok and Lema\{{\textbackslash}textasciicircum\}itre, Hervé and Penttilä, Jani and Conrod, Patricia J and Garavan, Hugh and Paillère Martinot, Marie-Laure and Martinot, Jean-Luc and {IMAGEN} consortium, Tobias and Flor, Herta and Fauth-Bühler, Mira and Poutska, Louise and Nees, Frauke and Grimmer, Yvonne and Struve, Maren and Heinz, Andeas and Ströhle, Andreas and Kappel, Viola and van Noort, Betteke Maria and Poline, Jean-Baptiste and Schwartz, Yanick and Thyreau, Benjamin and Ireland, James and Rogers, John and Bordas, Nadège and Bricaud, Zuleima and Filippi, Irina and Galinowski, André and Gollier-Briant, Fanny and Ménard, Vincent and Schumann, Gunter and Desrivières, Sylvane and Cattrell, Anna and Goodman, Robert and Stringaris, Argyris and Nymberg, Charlotte and Reed, Laurence and Barker, Gareth J and Ittermann, Berndt and Brühl, Ruediger and Smolka, Michael and Hübner, Thomas and Müller, Kathrin and Bokde, Arun L W and Büchel, Christian and Bromberg, Uli and Gallinat, Jurgen and Fadai, Tahmine and Gowland, Pennylaire and Lawrence, C and Paus, Tomas},
	date = {2017-02},
	note = {tex.bdsk-url-1: http://www.nature.com/articles/srep41678\%20http://www.ncbi.nlm.nih.gov/pubmed/28181512\%20http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid={PMC}5299428
tex.bdsk-url-2: https://dx.doi.org/10.1038/srep41678
tex.pmid: 28181512},
}

@article{Zois2017,
	title = {Frontal cortex gray matter volume alterations in pathological gambling occur independently from substance use disorder.},
	volume = {22},
	issn = {1369-1600},
	url = {http://doi.wiley.com/10.1111/adb.12368 http://www.ncbi.nlm.nih.gov/pubmed/26771165},
	doi = {10.1111/adb.12368},
	abstract = {Neuroimaging in pathological gambling ({PG}) allows studying brain structure independent of pharmacological/neurotoxic effects occurring in substance addiction. Because of high comorbidity of {PG} with substance use disorder ({SUD}), first results on structural deficits in {PG} are controversial. The current investigation is the first to examine gray matter ({GM}) volume alterations in {PG} controlling for the impact of {SUD} by comparing non-comorbid ({PGPURE} ) and two comorbid ({PGALCOHOL} and {PGPOLY} ) groups. Two hundred and five individuals were included in the analysis: 107 patients diagnosed with {PG} and 98 healthy controls ({HCs}). We employed voxel-based morphometry to look for {GM} volume differences between the groups controlling for age, smoking and depression. {GM} decreases in the superior medial and orbital frontal cortex occur independently of substance use in {PGPURE} compared with {HCs}. The frontal pattern of {GM} decrease was comparable with {PGALCOHOL} group where additionally {GM} volume was decreased in the anterior cingulate but increased in the amygdala. Moreover, regions in {PGALCOHOL} + {POLY} with reduced {GM} volume were the medial frontal, anterior cingulate and occipital lobe regions. {PGALCOHOL} + {POLY} not only exhibited structural deficits in comparison with {HCs} but also relative to {PGPURE} in the precuneus and post-central gyrus. We demonstrated specific frontal cortex {GM} deficits in {PG} without {SUD} comorbidities. Whereas some target regions reported in earlier studies might result from comorbid substance abuse, there seems to be a core set of frontal alterations associated with addicted gambling behaviour independent of toxic substance effects.},
	pages = {864--872},
	number = {3},
	journaltitle = {Addiction biology},
	author = {Zois, Evangelos and Kiefer, Falk and Lemenager, Tagrid and Vollstädt-Klein, Sabine and Mann, Karl and Fauth-Bühler, Mira},
	date = {2017},
	note = {tex.bdsk-url-1: http://doi.wiley.com/10.1111/adb.12368\%20http://www.ncbi.nlm.nih.gov/pubmed/26771165
tex.bdsk-url-2: https://dx.doi.org/10.1111/adb.12368
tex.pmid: 26771165},
	keywords = {behavioural addiction, gray matter, magnetic resonance imaging, pathological gambling, substance use comorbidity},
}

