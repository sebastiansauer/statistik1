@article{Goodman1999a,
	title = {Toward Evidence-Based Medical Statistics. 1: The P Value Fallacy},
	volume = {130},
	issn = {0003-4819},
	url = {http://annals.org/article.aspx?articleid=712762%5Cnhttp://www.ncbi.nlm.nih.gov/pubmed/10383371 http://annals.org/article.aspx?articleid=712762},
	doi = {10.7326/0003-4819-130-12-199906150-00008},
	abstract = {An important problem exists in the interpretation of modern medical research data: Biological understanding and previous research play little formal role in the interpretation of quantitative results. This phenomenon is manifest in the discussion sections of research articles and ultimately can affect the reliability of conclusions. The standard statistical approach has created this situation by promoting the illusion that conclusions can be produced with certain "error rates," without consideration of information from outside the experiment. This statistical approach, the key components of which are P values and hypothesis tests, is widely perceived as a mathematically coherent approach to inference. There is little appreciation in the medical community that the methodology is an amalgam of incompatible elements, whose utility for scientific inference has been the subject of intense debate among statisticians for almost 70 years. This article introduces some of the key elements of that debate and traces the appeal and adverse impact of this methodology to the P value fallacy, the mistaken idea that a single number can capture both the long-run outcomes of an experiment and the evidential meaning of a single result. This argument is made as a prelude to the suggestion that another measure of evidence should be used--the Bayes factor, which properly separates issues of long-run behavior from evidential strength and allows the integration of background knowledge with statistical findings.},
	pages = {995--1004},
	number = {12},
	journaltitle = {Annals of Internal Medicine},
	author = {Goodman, {SN} Steven N.},
	date = {1999},
	note = {tex.bdsk-url-1: http://annals.org/article.aspx?articleid=712762\%5Cnhttp://www.ncbi.nlm.nih.gov/pubmed/10383371\%20http://annals.org/article.aspx?articleid=712762
tex.bdsk-url-2: https://dx.doi.org/10.7326/0003-4819-130-12-199906150-00008
tex.isbn: 00034819
tex.pmid: 10383371},
	keywords = {Bayes Theorem, Humans, Probability, Research, Evidence-Based Medicine, Research: methods},
}

@article{Kocalevent2007,
	title = {Contribution to the construct validity of the Perceived Stress Questionnaire from a population-based survey.},
	volume = {63},
	issn = {0022-3999},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/17586340},
	doi = {10.1016/j.jpsychores.2007.02.010},
	abstract = {{OBJECTIVE}: Detailed evidence for the construct validity of stress questionnaires has been repeatedly demanded. This study aimed to investigate the construct validity of the Perceived Stress Questionnaire ({PSQ}) in the context of a transactional view of stress.

{METHODS}: The examination was based on the {PSQ} and on standardized quality of life and personality questionnaires. The analyses focused on structural equation modeling. A total of 2552 subjects from a population-based survey were studied.

{RESULTS}: A transactional model fitted the data. Personality aspects and resources contribute to the total perceived stress. Yet the physical aspects of quality of life receive a comparatively low weighting. The prevalence of perceived stress at a moderate level was estimated to be 14.5\%, lowest in the age group{\textgreater}75 years and highest in the 35- to 54-year age group. The prevalence of high stress was 3.1\%. The total {PSQ}-30 score of the general population was 0.30 (S.D.=0.15), slightly higher in women than in men.

{CONCLUSION}: We consider the {PSQ} as a valid instrument for recording subjective perceived stress in the context of a transactional view of stress. The present broad and international database suggests that it needs further investigation in terms of transcultural studies.},
	pages = {71--81},
	number = {1},
	journaltitle = {Journal of psychosomatic research},
	author = {Kocalevent, Rüya-Daniela and Levenstein, Susan and Fliege, Herbert and Schmid, Gabriele and Hinz, Andreas and Brähler, Elmar and Klapp, Burghard F},
	date = {2007-07},
	note = {tex.bdsk-url-2: https://dx.doi.org/10.1016/j.jpsychores.2007.02.010
tex.pmid: 17586340},
	keywords = {Humans, Adult, Aged, Male, Middle Aged, Quality of Life, Adaptation, Psychological, Questionnaires, Reproducibility of Results, Female, 80 and over, Neurotic Disorders, Neurotic Disorders: diagnosis, Neurotic Disorders: psychology, Personality Inventory, Personality Inventory: statistics \& numerical data, Psychological: complications, Psychometrics, Stress, Sex Factors, Psychometrics: statistics \& numerical data, Psychological: psychology, Age Factors, Self Efficacy, Quality of Life: psychology, Health Surveys, Somatoform Disorders, Somatoform Disorders: diagnosis, Somatoform Disorders: psychology},
