@report{davidson_when_2020,
	title = {When Psychometrics Fail: What are technology usage scales actually measuring?},
	url = {https://osf.io/6durk},
	shorttitle = {When Psychometrics Fail},
	abstract = {The mass adoption of digital technologies continues to generate psychological debate on how they impact people and society. A considerable body work has observed associations between technology use and a variety of negative outcomes including depression, anxiety, sleep, and cognitive impairments. However, the debate is far from settled with large, pre-registered studies failing to replicate these controversial findings. Unifying much of this work is the reliance on subjective estimates or self-reported smartphone ‘usage’ scales. These have repeatedly failed to align with objective behavior. As with other areas of psychometric assessment, researchers may now ask themselves what are these scales are actually measuring? Across three studies, we first observe that the majority of highly cited scales have not been developed in line with best psychometric practice. Next, using a principal component analysis ({PCA}), we then demonstrate that many scales are likely to be measuring a single, identical construct. Finally, we demonstrate an overlap between these scales and mental health, using both latent relationship ({EFA}) and data reduction ({PCA}) perspectives. These findings help explain why negative associations will always appear between technology use and well-being when using these measures. Our results suggest that psychologists need to critically consider how they proceed both methodologically and conceptually when developing psychometric scales if research findings are to ever be drawn together into a coherent body of knowledge.},
	institution = {{PsyArXiv}},
	type = {preprint},
	author = {Davidson, Brittany I and Shaw, Heather and Ellis, David Alexander},
	urldate = {2020-04-21},
	date = {2020-03-01},
	doi = {10.31234/osf.io/6durk},
}

