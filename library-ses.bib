@article{beckman_cognitive_2017,
	title = {Cognitive Transfer Outcomes for a Simulation-Based Introductory Statistics Curriculum},
	url = {http://arxiv.org/abs/1707.06537},
	abstract = {Cognitive transfer is the ability to apply learned skills and knowledge to new applications and contexts. This investigation evaluates cognitive transfer outcomes for a tertiary-level introductory statistics course using the {CATALST} curriculum, which exclusively used simulation-based methods to develop foundations of statistical inference. A common assessment instrument administered at the end of each course measured learning outcomes for students. {CATALST} students showed evidence of both near and far transfer outcomes while scoring as high, or higher on the assessed learning objectives, when compared with peers enrolled in similar courses that emphasized parametric inferential methods (e.g. the t-test).},
