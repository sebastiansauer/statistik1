	langid = {english},
	keywords = {Academic anxiety, Academic self-concept, Psychological assessment, Short scale, Single-item measure},
	file = {Eingereichte Version:/home/sebastian/Documents/Zotero/storage/HTD99X6L/Gogol et al. - 2014 - “My Questionnaire is Too Long!” The assessments of.pdf:application/pdf;ScienceDirect Snapshot:/home/sebastian/Documents/Zotero/storage/YRRH9UDZ/S0361476X14000204.html:text/html},
}

